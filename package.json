{
  "name": "Logins",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.4",
    "npm": "6.13.7"
  },
  "description": "A Simple Login Example using Node.js, Mongo and Redis",
  "main": "./server/app.js",
  "scripts": {
    "buildLoginBundle": "babel ./client/login ./client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "buildAppBundle": "babel ./client/app ./client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "build": "npm run buildLoginBundle && npm run buildAppBundle",
    "watchLoginBundle": "babel --watch ./client/login ./client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "watchAppBundle": "babel --watch ./client/app ./client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "nodemon": "nodemon -e js,html,css --watch ./server --watch ./hosted ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbm5733/DomoMaker-E.git"
  },
  "author": "Fisher Meddaugh",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/fbm5733/DomoMaker-E/issues"
  },
  "homepage": "https://github.com/fbm5733/DomoMaker-E=#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/compat-data": "^7.13.15",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "mongoose": "^5.12.3",
    "redis": "^3.1.0",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.13.0"
  }
}
